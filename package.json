{
  "name": "ts-express-starter",
  "version": "1.0.0",
  "description": "starter kit for typescript express",
  "main": "dist/server.js",
  "scripts": {
    "setup": "cp ./.env.example ./.env && npm install",
    "start": "ts-node-dev --respawn --transpile-only --watch ts,ejs,.env src/server.ts",
    "build": "rm -rf ./dist/* && tsc && cp -r ./src/views ./src/static ./dist",
    "serve": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "typescript",
    "express",
    "starter"
  ],
  "author": "Ismail",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "mongoose": "^6.2.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.1",
    "@types/ejs": "^3.0.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.18",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
